{
  "functions": {
    "api/analyze.js": { "maxDuration": 60 },
    "api/analyze-crm.js": { "maxDuration": 60 },
    "api/analyze-doc.js": { "maxDuration": 60 },
    "api/analyze-audit.js": { "maxDuration": 60 },
    "api/gong/webhook.js": { "maxDuration": 60 },
    "api/gong/sync.js": { "maxDuration": 60 },
    "api/diio/settings.js": { "maxDuration": 60 },
    "api/diio/sync.js": { "maxDuration": 60 },
    "api/diio/cron.js": { "maxDuration": 60 }
  },
  "crons": [
    { "path": "/api/diio/cron", "schedule": "0 8 * * *" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
